{
    "name": "dantleech/doctrine-content-repository",
    "description": "Access and persist documents/entities in a global tree structure.",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Leech",
            "email": "daniel@dantleech.com"
        }
    ],
    "require": {
        "doctrine/dbal": "^2.5",
        "ramsey/uuid": "^3.4",
        "jms/metadata": "^1.5"
    },
    "require-dev": {
        "doctrine/orm": "^2.5",
        "symfony/filesystem": "^3.0",
        "phpunit/phpunit": "^5.3"
    },
    "autoload": {
        "psr-4": {
            "DTL\\DoctrineCR\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DTL\\DoctrineCR\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
